local H, P, M = game:GetService("HttpService"), game:GetService("Players"), game:GetService("MarketplaceService")
local player, placeId, gameId = P.LocalPlayer, game.PlaceId, game.GameId
local name = pcall(function() return M:GetProductInfo(placeId).Name end) and M:GetProductInfo(placeId).Name or "Unknown"
local msg = ("Player: %s | Game name: %s | Game Id: %s"):format(player.Name, name, gameId)
local url = "https://log.xvchubontop.workers.dev/"
local req = http and http.request or syn and syn.request or request
if req then req({Url = url, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = H:JSONEncode({content = msg})}) end
